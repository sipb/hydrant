            "partLab": {
              "type": "boolean"
            },
            "lectureUnits": {
              "type": "integer"
            },
            "labUnits": {
              "type": "integer"
            },
            "preparationUnits": {
              "type": "integer"
            },
            "level": {
              "type": "string"
            },
            "isVariableUnits": {
              "type": "boolean"
            },
            "same": {
              "type": "string"
            },
            "meets": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "inCharge": {
              "type": "string"
            },
            "virtualStatus": {
              "type": "boolean"
