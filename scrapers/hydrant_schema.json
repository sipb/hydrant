            "final": {
              "type": "boolean"
            },
            "half": {
              "enum": [false, 1, 2]
            },
            "limited": {
              "type": "boolean"
            },
            "cim": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "required": [
            "number",
            "course",
            "subject",
            "terms",
            "prereqs",
            "tba",
            "sectionKinds",
            "hassH",
            "hassA",
            "hassS",
            "hassE",
            "cih",
            "cihw",
            "rest",
            "lab",
            "partLab",
            "lectureUnits",
            "labUnits",
            "preparationUnits",
            "level",
            "isVariableUnits",
            "same",
            "meets",
            "description",
            "name",
            "inCharge",
            "virtualStatus",
            "rating",
            "hours",
            "size",
            "nonext",
            "repeat",
            "url",
            "final",
            "half",
            "limited"
