    "termInfo": {
      "type": "object",
      "properties": {
        "urlName": {
          "type": "string"
        },
        "startDate": {
          "type": "string",
          "format": "date"
        },
        "h1EndDate": {
          "type": "string",
          "format": "date"
        },
        "h2StartDate": {
          "type": "string",
          "format": "date"
        },
        "endDate": {
          "type": "string",
          "format": "date"
        },
        "mondayScheduleDate": {
          "type": "null"
        },
        "holidayDates": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date"
          }
        }
      },
      "required": [
        "urlName",
        "startDate",
        "h1EndDate",
        "h2StartDate",
        "endDate",
        "mondayScheduleDate",
        "holidayDates"
      ]
    },
    "lastUpdated": {
      "type": "string",
      "format": "date"
    },
    "classes": {
