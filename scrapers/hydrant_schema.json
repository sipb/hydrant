              }
            },
            "prereqs": {
              "type": "string"
            },
            "sectionKinds": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "lectureRawSections": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "lectureSections": {
              "type": "array",
              "items": {
                "type": "array",
                "items": {
                  "type": ["array", "string"],
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "integer"
                    }
                  }
                }
              }
            },
            "recitationSections": {
              "type": "array",
              "items": {
                "type": "array"
              }
            },
            "labSections": {
              "type": "array",
              "items": {
                "type": "array"
              }
            },
            "designSections": {
              "type": "array",
              "items": {
                "type": "array"
              }
            },
            "recitationRawSections": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "labRawSections": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "designRawSections": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "tba": {
              "type": "boolean"
            },
            "hassH": {
              "type": "boolean"
            },
            "hassA": {
              "type": "boolean"
            },
            "hassS": {
              "type": "boolean"
            },
            "hassE": {
              "type": "boolean"
            },
            "cih": {
              "type": "boolean"
            },
            "cihw": {
              "type": "boolean"
            },
            "rest": {
              "type": "boolean"
            },
            "lab": {
              "type": "boolean"
